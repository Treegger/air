<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx">

	<fx:Script>
		<![CDATA[
			import com.treegger.airim.controller.Contact;
			
			[Bindable]
			public var color:uint;
			
		]]>
	</fx:Script>
	
	<s:Ellipse left="10" top="3" width="10" height="10">
		<s:fill>
			<s:SolidColor color="{color}" alpha="1" />
		</s:fill>			
		<s:filters>
			<s:BevelFilter angle="45" distance="5"
						   highlightAlpha="0.8"
						   highlightColor="0xffffff"
						   shadowAlpha="0.4"
						   shadowColor="0x000000"
						   type="inner"/>
		</s:filters>
	</s:Ellipse>
	<s:Ellipse left="10" top="3" width="10" height="10">
		<s:stroke>
			<s:SolidColorStroke color="0x000000" alpha="0.3" weight="1"/>
		</s:stroke>
	</s:Ellipse>
	
	
</s:Group>
